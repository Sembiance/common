{
	"blocks"                 : false,
	"brackets"               : "never",
	"colons"                 : "never",
	"colors"                 : false,
	"commaSpace"             : "always",
	"commentSpace"           : false,
	"cssLiteral"             : "never",
	"customProperties"       : ["transition-box-shadow", "background-linear-gradient"],
	"depthLimit"             : 8,
